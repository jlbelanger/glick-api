{
	"name": "jlbelanger/glick-api",
	"description": "Track occurrences of time-based events.",
	"version": "1.0.0",
	"type": "project",
	"license": "GPL-3.0-only",
	"authors": [
		{
			"name": "Jenny Belanger",
			"email": "mail@jennybelanger.com",
			"homepage": "https://www.jennybelanger.com/"
		}
	],
	"require": {
		"php": "^7.3|^8.0",
		"fruitcake/laravel-cors": "^3.0",
		"guzzlehttp/guzzle": "^7.5",
		"jlbelanger/tapioca": "dev-main#c7e09fb7d05f900d4ba85a39fefef61af58dc412",
		"laravel/framework": "^8.83.27",
		"laravel/sanctum": "^2.15.1"
	},
	"require-dev": {
		"facade/ignition": "^2.17.6",
		"fakerphp/faker": "^1.21",
		"mockery/mockery": "^1.5.1",
		"nunomaduro/collision": "^5.11",
		"phpunit/phpunit": "^9.5.27",
		"squizlabs/php_codesniffer": "^3.7.1"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/",
			"Database\\Factories\\": "database/factories/",
			"Database\\Seeders\\": "database/seeders/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:jlbelanger/tapioca.git"
		}
	],
	"config": {
		"discard-changes" : true,
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"scripts": {
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover --ansi"
		],
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"@php artisan key:generate --ansi"
		]
	},
	"extra": {
		"laravel": {
			"dont-discover": []
		}
	}
}
